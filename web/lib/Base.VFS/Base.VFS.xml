<?xml version="1.0" encoding="utf-8" standalone="no"?>
<actions>
    <action name="VfsRpcService"/>
    <action name="UploadFile">
        <path>UploadFileAction</path>
        <parameters>
            <request>
                <param name="ufa_Resizable">false</param>
                <param name="ufa_Settings">
                    array(
                        "prefix"  => "original_"
                        , "keep"  => true
                        , "modes" => array(
                            array(
                                "prefix"   => "small_"
                                , "width"  => 93
                                , "height" => 93
                                , "scale"  => false
                                , "quality"=> 95
                            ), array(
                                "prefix"   => "big_"
                                , "width"  => 200
                                , "height" => 200
                                , "scale"  => false
                                , "quality"=> 95
                            )
                        )
                    );
                </param>
            </request>
        </parameters>
    </action>
</actions>